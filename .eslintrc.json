{
    "root": true,
    "extends": [
      "eslint:recommended",
      "plugin:import/errors",
      "plugin:import/warnings"
    ],
    "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module"
    },
    "env": {
      "node": true,
      "es6": true,
      "mocha": true,
      "commonjs": true
    },
    "rules": {
        "no-console": 1
    }
  }
